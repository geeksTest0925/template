{
  "name": "front3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "cross-env OUTPUT_DIR=background-template vue-cli-service build",
    "build:prod": "cross-env OUTPUT_DIR=background-template vue-cli-service build",
    "build:stage": "cross-env OUTPUT_DIR=background-template vue-cli-service build --mode stage",
    "deploy:stage": "npm run build:stage && cross-env NODE_ENV=stage OUTPUT_DIR=background-template node ../cmd-deploy-config",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "js-md5": "^0.7.3",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "qrcode": "^1.5.1",
    "qs": "^6.10.3",
    "three": "0.135.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "^3.2.6",
    "ant-design-vue": "^3.2.0",
    "babel-plugin-import": "^1.13.3",
    "css-loader": "^6.5.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "mitt": "^3.0.0",
    "moment": "^2.29.1",
    "js-cookie": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
